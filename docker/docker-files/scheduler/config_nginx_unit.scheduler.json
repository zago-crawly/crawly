{
  "listeners": {
    "*:80": {
      "pass": "applications/scheduler_api"
    }
  },
  "applications": {
    "scheduler_api": {
      "type": "python 3.10",
      "working_directory": "/usr/src/scheduler_api",
      "path": "/usr/src/scheduler_api/src/scheduler/api/",
      "home": "/usr/src/scheduler_api/.venv/",
      "module": "scheduler_api_svc",
      "callable": "app",
      "environment": {
          "log": "{\"level\": \"INFO\", \"file_name\": \"log/scheduler_api.log\", \"retention\": \"1 months\", \"rotation\": \"20 days\"}"
      }
    },
    "scheduler_app": {
      "type": "python 3.10",
      "working_directory": "/usr/src/scheduler_app",
      "path": "/usr/src/scheduler_app/src/scheduler/app/",
      "home": "/usr/src/scheduler_app/.venv/",
      "module": "scheduler_app_svc",
      "callable": "app",
      "environment": {
        "log": "{\"level\": \"INFO\", \"file_name\": \"log/scheduler_app.log\", \"retention\": \"1 months\", \"rotation\": \"20 days\"}"
      }
    }
  }
}